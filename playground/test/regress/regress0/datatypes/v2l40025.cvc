% EXPECT: valid
% EXIT: 20
DATATYPE
  nat = succ(pred : nat) | zero,
  list = cons(car : tree, cdr : list) | null,
  tree = node(children : list) | leaf(data : nat)
END;

x1 : nat ;
x2 : nat ;
x3 : list ;
x4 : list ;
x5 : tree ;
x6 : tree ;

QUERY 

(NOT (((is_leaf(x5)
 AND (NOT (x2 = x1)))
 AND (x2 = pred(pred(zero))))
 AND (NOT is_node(node(null)))));
