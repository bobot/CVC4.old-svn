% EXPECT: invalid
% EXIT: 10
DATATYPE
  nat = succ(pred : nat) | zero,
  list = cons(car : tree, cdr : list) | null,
  tree = node(children : list) | leaf(data : nat)
END;

x1 : nat ;
x2 : nat ;
x3 : list ;
x4 : list ;
x5 : tree ;
x6 : tree ;

QUERY 

(NOT ((is_node(x6)
 AND (x6 = x5))
 AND (NOT (null = cons(x6,x3)))));
