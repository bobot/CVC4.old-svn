#
# Option specification file for CVC4
# See src/options/options for a description of this file format
#

module PROP "prop/options.h" Propositional

option satRandomFreq random-frequency --random-freq=P double :default 0.0 :predicate greater_equal(0.0) less_equal(1.0)
 sets the frequency of random decisions in the sat solver(P=0.0 by default)
option satRandomSeed random-seed --random-seed=S double :default 91648253
 sets the random seed for the sat solver

endmodule
