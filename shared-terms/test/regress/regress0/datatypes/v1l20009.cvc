% EXPECT: valid
% EXIT: 20
DATATYPE
  nat = succ(pred : nat) | zero,
  list = cons(car : tree, cdr : list) | null,
  tree = node(children : list) | leaf(data : nat)
END;

x1 : nat ;
x2 : list ;
x3 : tree ;

QUERY 

(NOT ((NOT is_zero(pred(succ(pred(zero)))))
 AND (data(x3) = succ(pred(data(leaf(succ(data(car(null))))))))));
